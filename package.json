{
  "name": "@parsyl/claims-ts-client",
  "version": "1.0.0",
  "description": "TS Client for the post bind claims-api ",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parsyl/claims-ts-client.git"
  },
  "author": "Arnold Lei",
  "license": "MIT",
  "private": false,
  "scripts": {
    "generate-client": "sh openapi-client.sh",
